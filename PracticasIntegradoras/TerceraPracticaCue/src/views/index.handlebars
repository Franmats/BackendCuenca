<h2>Comerce</h2>

     <table>
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Precio</th>
                <th>Stock</th>
            </tr>

        </thead>
        <tbody>
            {{#each docs}}
                <tr>
                    <td>{{this.nombre}}</td>
                    <td>{{this.lprecio}}</td>
                    <td>{{this.Stock}}</td>
                                   </tr>
            {{/each}}
        </tbody>
     </table>

     <div>
        {{#if hasPrevPage }}
            <a href="{{prevLink}}">Anterior</a>
        {{/if}}
        {{#if hasNextPage}}
            <a href="{{nextLink}}">Siguiente</a>  
        {{/if}}      
     </div>
